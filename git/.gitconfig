[alias]
  co = checkout
  ct = commit
  st = status
  br = branch
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[credential]
	helper = cache --timeout=43200

[branch]
    sort=-committerdate
	autosetuprebase = always
